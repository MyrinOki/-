class ProcessDictionaryMaker:
    def __init__(self, schedule_data):
        self.job_dictionary = {}
        self.resource_dictionary = {}
        self.distance_dictionary = {}
        self.schedule_data = schedule_data

    def make_process_dictionary(self):
        for key, data in self.schedule_data.items():
            if key[0] == 'J':
                self.job_dictionary[key] = data
            elif key[0] == 'o':
                self.distance_dictionary[key] = data
            else:
                self.resource_dictionary[key] = data
        return self

# 使用例
schedule_data = {
    # 'J'で始まるキーはジョブデータ
    'J1': 'ジョブデータ1',
    # 'o'で始まるキーは距離データ
    'o1': '距離データ1',
    # その他のキーはリソースデータ
    'R1': 'リソースデータ1'
}
process_maker = ProcessDictionaryMaker(schedule_data)
process_maker.make_process_dictionary()

print("Job Dictionary:", process_maker.job_dictionary)
print("Resource Dictionary:", process_maker.resource_dictionary)
print("Distance Dictionary:", process_maker.distance_dictionary)
